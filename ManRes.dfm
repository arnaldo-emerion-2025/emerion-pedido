  object UpRes: TUpdateSQL
    ModifySQL.Strings = (
      'update PedRes'
      'set'
      '  SITRES = :SITRES'
      'where'
      '  CODEMP = :OLD_CODEMP and'
      '  DTERES = :OLD_DTERES and'
      '  NUMRES = :OLD_NUMRES')
    InsertSQL.Strings = (
      'insert into PedRes'
      
        '  (CODEMP, DTERES, NUMRES, HRERES, DTFRES, UFERES, CGCCLI, PEDAN' +
        'T, '
      'QTIRES, '
      
        '   QTPRES, LANEST, CODFIL, CODCLI, CODTRA, CODTIP, CODVEN, CODPF' +
        'A, '
      'TIPPFA, '
      
        '   CODATD, TOTRES, TOTSUB, TOTGER, TOTCST, MEDDSC, TOTDSC, MEDAC' +
        'R, '
      'TOTACR, '
      '   MEDPRM, TOTDSP, MEDCOM, TOTCOM, MEDDCO, TOTDCO, DSCREG, '
      'TOTDSR, TOTIPI, '
      
        '   FATGER, DEVGER, SLDGER, TOTREN, USUREJ, FLGOCO, FLGIMP, ATUES' +
        'T, '
      'SITRES)'
      'values'
      
        '  (:CODEMP, :DTERES, :NUMRES, :HRERES, :DTFRES, :UFERES, :CGCCLI' +
