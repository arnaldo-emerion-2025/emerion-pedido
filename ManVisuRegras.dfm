  object EstUfe: TwwQuery
    Active = True
    CachedUpdates = True
    DatabaseName = 'ISade'
    DataSource = DsStr
    SQL.Strings = (
      'Select EstUfe.*,'
      '           GerUfe.NomUfe'
      'From EstUfe LEFT JOIN GerUfe ON (EstUfe.SigUfe = GerUfe.SigUfe)'
      'Where EstUfe.CodStr = :CodStr'
      '    and EstUfe.TipStr = :TipStr'
      'Order by EstUfe.SigUfe')
    ValidateWithMask = True
    Left = 73
    Top = 433
    ParamData = <
      item
        DataType = ftFixedChar
