  object PedCt2: TwwQuery
    CachedUpdates = True
    DatabaseName = 'ISade'
    DataSource = DsCta
    SQL.Strings = (
      'Select PedCt2.CodEmp,'
      '           PedCt2.DteCta,'
      '           PedCt2.NumCta,'
      '           PedCt2.CodGru,'
      '           PedCt2.CodSub,'
      '           PedCt2.CodPro,'
      '           PedCt2.DesCt2,'
      '           PedCt2.ObsCt2,'
      '           PedCt2.QtpCt2,'
      '           PedCt2.CodUnd,'
      '           PedCt2.VluCt2,'
      '           PedCt2.VcsCt2,'
      '           PedCt2.DscCt2,'
      '           PedCt2.VlqCt2,'
      '           PedCt2.IcmCt2,'
      '           PedCt2.totsub,'
      '           PedCt2.TotCt2,'
      '           PedCt2.TotCst,'
      '           PedCt2.TotRen,'
      '           EstMrc.NomMrc'
      'From PedCt2 LEFT JOIN EstPro ON (PedCt2.CodClp = EstPro.CodClp)'
      
        '                                                 AND (PedCt2.Cod' +
        'Gru = EstPro.CodGru)'
      
        '                                                 AND (PedCt2.Cod' +
        'Sub = EstPro.CodSub)'
      
        '                                                 AND (PedCt2.Cod' +
        'Pro = EstPro.CodPro)'
      
        '                     LEFT JOIN EstMrc ON (EstPro.CodMrc = EstMrc' +
        '.CodMrc)'
      'Where PedCt2.CodEmp = :CodEmp'
      '     and PedCt2.DteCta = :DteCta'
      '     and PedCt2.NumCta = :NumCta'
      'Order by PedCt2.NroCt2')
    ValidateWithMask = True
    Left = 72
    Top = 385
    ParamData = <
      item
        DataType = ftInteger
        Name = 'CODEMP'
        ParamType = ptInput
      end
      item
        DataType = ftDateTime
        Name = 'DTECTA'
        ParamType = ptInput
      end
      item
        DataType = ftInteger
        Name = 'NUMCTA'
        ParamType = ptInput
      end>
    object PedCt2CODITE: TStringField
      DisplayLabel = 'Item'
      DisplayWidth = 18
      FieldKind = fkCalculated
      FieldName = 'CODITE'
      OnGetText = PedCt2CODITEGetText
      Calculated = True
    end
