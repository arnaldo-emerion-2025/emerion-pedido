  object quSql: TwwQuery
    DatabaseName = 'ISade'
    UniDirectional = True
    ValidateWithMask = True
    Left = 34
    Top = 413
  end
  object UpCta: TUpdateSQL
    ModifySQL.Strings = (
      'update PedCta'
      'set'
      '  SITCTA = :SITCTA'
      'where'
      '  CODEMP = :OLD_CODEMP and'
      '  DTECTA = :OLD_DTECTA and'
      '  NUMCTA = :OLD_NUMCTA')
    InsertSQL.Strings = (
      'insert into PedCta'
      
        '(CODEMP, DTECTA, NUMCTA, HRECTA, CODCLI, NOMCLI, CODVEN, CODATD,' +
        ' '
      'CODPFA, TOTITE, TOTGER, TOTCST, TOTREN, NUMRES, SITCTA)'
      'values'
      '(:CODEMP, :DTECTA, :NUMCTA, :HRECTA, :CODCLI, :NOMCLI, :CODVEN, '
      ':CODATD, :CODPFA, :TOTITE, :TOTGER, :TOTCST, :TOTREN, :NUMRES, '
      ':SITCTA)')
    DeleteSQL.Strings = (
      'delete from PedCta'
      'where'
      '  CODEMP = :OLD_CODEMP and'
      '  DTECTA = :OLD_DTECTA and'
      '  NUMCTA = :OLD_NUMCTA')
    Left = 6
    Top = 413
  end
  object PopupMenu1: TPopupMenu
    Left = 336
    Top = 256
