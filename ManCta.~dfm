      Caption = 'Exportar dados em Excel'
      OnClick = ExportardadosemExcel1Click
    end
    object Exportaordemdeincluso1: TMenuItem
      Caption = 'Exporta ordem de inclusão'
      OnClick = Exportaordemdeincluso1Click
    end
  end
end
