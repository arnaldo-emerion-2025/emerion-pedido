        'T, OBSFIN, '
      
        '   SITRES, CLITAB, CODTCL, NOMENT, OB1FAT, OB2FAT, OB3FAT, OB4FA' +
        'T, OB5FAT, '
      
        '   OB6FAT, OB7FAT, OB8FAT, DSCRES, ID_FINUFE, ID_FINCIE, FRTRES,' +
        ' TOTFRT, '
      
        '   NROSUF, TIPFRT, PERPIS, PERCOF, TOTSEG, TOTOUTDESP, TOTDESCIN' +
        'C, ID_FRETE, '
      
        '   SUFR_ICMS, SUFR_IPI, SUFR_PIS, SUFR_COFINS, DTVSUF, REGTRB, I' +
        'NDIC_CF, '
      '   INDIC_PRESENCA, APP_VER)'
      'values'
      
        '  (:CODEMP, :DTERES, :NUMRES, :HRERES, :CODCLI, :CODVEN, :CODPFA' +
        ', :TIPPFA, '
      
        '   :CODTIP, :PRFRES, :DTFRES, :CODATD, :CODTRA, :FLGTAB, :UFERES' +
        ', :CGCCLI, '
      
        '   :INSCLI, :CEPCLI, :TENCLI, :ENDCLI, :REFCLI, :NUMCLI, :BAICLI' +
        ', :CIDCLI, '
      
