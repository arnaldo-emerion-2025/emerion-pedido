      '  ID_FINCIE = :ID_FINCIE,'
      '  FRTRES = :FRTRES,'
      '  TOTFRT = :TOTFRT,'
      '  NROSUF = :NROSUF,'
      '  TIPFRT = :TIPFRT,'
      '  PERPIS = :PERPIS,'
      '  PERCOF = :PERCOF,'
      '  TOTSEG = :TOTSEG,'
      '  TOTOUTDESP = :TOTOUTDESP,'
      '  TOTDESCINC = :TOTDESCINC,'
      '  ID_FRETE = :ID_FRETE,'
      '  SUFR_ICMS = :SUFR_ICMS,'
      '  SUFR_IPI = :SUFR_IPI,'
      '  SUFR_PIS = :SUFR_PIS,'
      '  SUFR_COFINS = :SUFR_COFINS,'
      '  DTVSUF = :DTVSUF,'
      '  REGTRB = :REGTRB,'
      '  INDIC_CF = :INDIC_CF,'
      '  INDIC_PRESENCA = :INDIC_PRESENCA,'
      '  APP_VER = :APP_VER'
      'where'
      '  CODEMP = :OLD_CODEMP and'
      '  DTERES = :OLD_DTERES and'
      '  NUMRES = :OLD_NUMRES')
    InsertSQL.Strings = (
      'insert into PedRes'
      
        '  (CODEMP, DTERES, NUMRES, HRERES, CODCLI, CODVEN, CODPFA, TIPPF' +
        'A, CODTIP, '
      
        '   PRFRES, DTFRES, CODATD, CODTRA, FLGTAB, UFERES, CGCCLI, INSCL' +
        'I, CEPCLI, '
      
        '   TENCLI, ENDCLI, REFCLI, NUMCLI, BAICLI, CIDCLI, UFECLI, INECL' +
        'I, CGECLI, '
      
