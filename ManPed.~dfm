  object UpRe2Fec: TUpdateSQL
    ModifySQL.Strings = (
      'update PedRe2'
      'set'
      '  CODEMP = :CODEMP,'
      '  VLURE2 = :VLURE2,'
      '  VLQRE2 = :VLQRE2,'
      '  DSCRE2 = :DSCRE2,'
      '  VDSRE2 = :VDSRE2,'
      '  DSPRE2 = :DSPRE2,'
      '  VDPRE2 = :VDPRE2,'
      '  DSRRE2 = :DSRRE2,'
      '  VDRRE2 = :VDRRE2,'
      '  PACRE2 = :PACRE2,'
      '  VACRE2 = :VACRE2'
      'where'
      '  CODEMP = :OLD_CODEMP and'
      '  DTERES = :OLD_DTERES and'
      '  NUMRES = :OLD_NUMRES and'
      '  SEQRE2 = :OLD_SEQRE2')
    InsertSQL.Strings = (
      'insert into PedRe2'
      
        '  (CODEMP, VLURE2, VLQRE2, DSCRE2, VDSRE2, DSPRE2, VDPRE2, DSRRE' +
        '2, VDRRE2, '
      '   PACRE2, VACRE2)'
      'values'
      
        '  (:CODEMP, :VLURE2, :VLQRE2, :DSCRE2, :VDSRE2, :DSPRE2, :VDPRE2' +
        ', :DSRRE2, '
      '   :VDRRE2, :PACRE2, :VACRE2)')
    DeleteSQL.Strings = (
      'delete from PedRe2'
      'where'
      '  CODEMP = :OLD_CODEMP and'
      '  DTERES = :OLD_DTERES and'
      '  NUMRES = :OLD_NUMRES and'
      '  SEQRE2 = :OLD_SEQRE2')
    Left = 62
    Top = 343
  end
