  object quSql: TwwQuery
    DatabaseName = 'ISade'
    UniDirectional = True
    ValidateWithMask = True
    Left = 2
    Top = 371
  end
  object PedRe2: TwwQuery
    CachedUpdates = True
    BeforeInsert = PedRe2BeforeInsert
    BeforeEdit = PedRe2BeforeEdit
    BeforePost = PedRe2BeforePost
    AfterScroll = PedRe2AfterScroll
    OnCalcFields = PedRe2CalcFields
    OnNewRecord = PedRe2NewRecord
    DatabaseName = 'ISade'
    DataSource = DsRes
    SQL.Strings = (
      'Select RTRIM(LTRIM(PedRe2.ClsIPI)) ClsIPI, PedRe2.* From PedRe2'
      'Where PedRe2.CodEmp = :CodEmp'
      '     and PedRe2.DteRes = :DteRes'
