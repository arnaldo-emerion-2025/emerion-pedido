      '  INECLI = :INECLI,'
      '  CGECLI = :CGECLI,'
      '  OBSRES = :OBSRES,'
      '  DSCREG = :DSCREG,'
      '  DSCCOM = :DSCCOM,'
      '  TOTDSR = :TOTDSR,'
      '  CODUSU = :CODUSU,'
      '  FLGAVI = :FLGAVI,'
