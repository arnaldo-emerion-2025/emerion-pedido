  object quRAF: TwwQuery
    DatabaseName = 'ISade'
    SQL.Strings = (
      'Select'
      ''
      '        ROUND(('
      '        (SUM('
      '           case'
      '              when ((FlgDel <> '#39'*'#39') and (FlgDev <> '#39'*'#39')) then'
      '                 case'
      '                    when (FlpCrp <> '#39'*'#39') then'
      '                       case'
      
        '                         when (DTVCRP < (SELECT CAST('#39'TODAY'#39' AS ' +
        'DATE) FROM RDB$DATABASE)) then'
      
        '                            (SELECT CAST('#39'TODAY'#39' AS DATE) FROM R' +
        'DB$DATABASE) - DtvCrp'
      '                         else'
      '                            0'
      '                       end'
      '                    else'
      '                       case'
      
        '                          when DulCrp is not null then (DulCrp -' +
        ' DtvCrp)'
      '                          else '#39'1899-12-28'#39' - DtvCrp'
      '                       end'
      '                 end'
      '           end'
      '        ))'
      '        /'
      '        SUM('
      '           case'
      '              when ((FlgDel <> '#39'*'#39') and (FlgDev <> '#39'*'#39')) then'
      '                 case'
      '                    when (FlpCrp <> '#39'*'#39') then'
      '                       case'
      
        '                          when (DTVCRP < (SELECT CAST('#39'TODAY'#39' AS' +
        ' DATE) FROM RDB$DATABASE)) then 1'
      '                          else 0'
      '                       end'
      '                    else'
      '                       case'
      '                          when (DulCrp > DtvCrp) then 1'
      '                          else 0'
      '                       end'
      '                 end'
      '           end'
      '        )'
      '    ), 2) as ATRASO_MEDIO,'
      ''
      '    SUM(FinCrp.VlpCrp) FATURADO,'
      '    SUM(FinCrp.TotCrp) RECEBIDO,'
      '    SUM('
      '        case'
      
        '         when ((FlgDel <> '#39'*'#39') and (FlgDev <> '#39'*'#39') and (FlpCrp <' +
        '> '#39'*'#39'))'
      '            then VlsCrp'
      '            else 0'
      '      end) VLR_EM_ABERTO,'
      '    SUM('
      '        --case FLPCRP'
      '        case'
      
        '            when ((FlgDel <> '#39'*'#39') and (FlgDev <> '#39'*'#39') and (FlpCr' +
        'p <> '#39'*'#39')) then 1'
      '            else 0'
      '        end'
      '    ) QTD_EM_ABERTO,'
      ''
      '    SUM('
      '      case'
      '         when ((FlgDel = '#39'*'#39') or (FlgDev = '#39'*'#39'))'
      '            then VlpCrp'
      '            else 0'
      '      end'
      '    )CANCELADO_DEVOLVIDO,'
      ''
      '    SUM('
      '        case'
      '            when ((FlgDel = '#39'*'#39') or (FlgDev = '#39'*'#39')) then 1'
      '            else 0'
      '        end'
      '    )QTD_CANCELADO_DEVOLVIDO,'
      ''
      '    (Select coalesce(Sum(ValChc),0) From FinChc'
      
        '        Where CodCli = :CODCLI and SitChc <> '#39'Compensado'#39' and Si' +
        'tChc <> '#39'Devolvido'#39
      '    ) CHEQUE_PENDENTE,'
      ''
      '    (Select coalesce(Sum(ValChc),0) From FinChc'
      '        Where CodCli = :CODCLI and SitChc = '#39'Devolvido'#39
      '    ) CHEQUE_DEVOLVIDO,'
      ''
      '    SUM('
      '        case'
      '         when ((FlgDel <> '#39'*'#39') and (FlgDev <> '#39'*'#39'))'
      '            then VlsCrp'
      '            else 0'
      '      end'
      '    ) TOT_RECEBER,'
      ''
      '    SUM('
      '      case'
      '         when FlgInc = '#39'Sim'#39
      '            then vlscrp'
      '            else 0'
      '      end'
      '    )TOT_INCOBRAVEL,'
      ''
      '    SUM('
      '      case'
      '         when FlgInc = '#39'Sim'#39
      '            then 1'
      '            else 0'
      '      end'
      '    )QTD_TOT_INCOBRAVEL,'
      ''
      '    SUM('
      '      case'
      '         when FlrCrp = '#39'*'#39
      '            then vlpcrp'
      '            else 0'
      '      end'
      '    )TIT_DESC,'
      ''
      
        '    SUM(Round(FinCrp.VjpCrp + FinCrp.VtpCrp,2)) as TOTAL_ACRESCI' +
        'MO,'
      ''
      '    SUM(VDSCRP) TOT_DESCONTO,'
      ''
      '    SUM('
      '        case'
      '            when ((FlgDel <> '#39'*'#39') and (FlgDev <> '#39'*'#39')) then'
      '                case'
      
        '                   when ((FlpCrp <> '#39'*'#39') and (DTVCRP < (SELECT C' +
        'AST('#39'TODAY'#39' AS DATE) FROM RDB$DATABASE))) then VLPCRP'
      '                   else 0'
      '                end'
      '            else 0'
      '        end'
      '    ) TOTAL_EM_ATRASO,'
      ''
      '    SUM('
      '        case'
      '            when ((FlgDel <> '#39'*'#39') and (FlgDev <> '#39'*'#39')) then'
      '                case'
      
        '                   when ((FlpCrp <> '#39'*'#39') and (DTVCRP < (SELECT C' +
        'AST('#39'TODAY'#39' AS DATE) FROM RDB$DATABASE))) then 1'
      '                   else 0'
      '                end'
      '            else 0'
      '        end'
      '    ) QTD_TOTAL_EM_ATRASO,'
      ''
      '    MIN(DTECRE) PESQUISADO_DESDE,'
      
        '    (((MAX(DTECRE) - MIN(DTECRE)) - MOD((MAX(DTECRE) - MIN(DTECR' +
        'E)), 30)) / 30) as PERIODO_PESQ,'
      ''
      '    MAX(TOTCRE) AS MAIOR_COMPRA,'
      
        '    (select first 1 cre.dtecre from FinCrp,FinCre cre,FinBan,Ger' +
        'Emp'
      '        where cre.TOTCRE ='
      '           (select MAX(fincre.TOTCRE)'
      '               From FinCrp,FinCre,FinBan,GerEmp'
      '                Where'
      '                    FinCrp.CodEmp = FinCre.CodEmp   and'
      '                    FinCrp.DteCre = FinCre.DteCre   and'
      '                    FinCrp.NumCre = FinCre.NumCre   and'
      
        '                    coalesce(FinCrp.CodBan,888) = FinBan.CodBan ' +
        '  and'
      '                    FinCrp.CodEmp = GerEmp.CodEmp   and'
      '                    FinCrp.CodCli = :CODCLI'
      '                    and ('
      
        '                            ( FinCrp.FlgDev = '#39' '#39' and FinCrp.Flg' +
        'Del = '#39' '#39' ) or'
      '                            ( FinCrp.FlpCrp = '#39'*'#39' )'
      '                        )'
      '           )'
      ''
      '        and FinCrp.CodEmp = FinCre.CodEmp   and'
      '                    FinCrp.DteCre = FinCre.DteCre   and'
      '                    FinCrp.NumCre = FinCre.NumCre   and'
      
        '                    coalesce(FinCrp.CodBan,888) = FinBan.CodBan ' +
        '  and'
      '                    FinCrp.CodEmp = GerEmp.CodEmp   and'
      '                    FinCrp.CodCli = :CODCLI'
      '                    and ('
      
        '                            ( FinCrp.FlgDev = '#39' '#39' and FinCrp.Flg' +
        'Del = '#39' '#39' ) or'
      '                            ( FinCrp.FlpCrp = '#39'*'#39' )'
      '                        )'
      '        order by numcre'
      '    ) MAIOR_COMPRA_DATA,'
      ''
      '    MIN(TOTCRE) AS MENOR_COMPRA,'
      
        '    (select first 1 cre.dtecre from FinCrp,FinCre cre,FinBan,Ger' +
        'Emp'
      '        where cre.TOTCRE ='
      '           (select MIN(fincre.TOTCRE)'
      '               From FinCrp,FinCre,FinBan,GerEmp'
      '                Where'
      '                    FinCrp.CodEmp = FinCre.CodEmp   and'
      '                    FinCrp.DteCre = FinCre.DteCre   and'
      '                    FinCrp.NumCre = FinCre.NumCre   and'
      
        '                    coalesce(FinCrp.CodBan,888) = FinBan.CodBan ' +
        '  and'
      '                    FinCrp.CodEmp = GerEmp.CodEmp   and'
      '                    FinCrp.CodCli = :CODCLI'
      '                    and ('
      
        '                            ( FinCrp.FlgDev = '#39' '#39' and FinCrp.Flg' +
        'Del = '#39' '#39' ) or'
      '                            ( FinCrp.FlpCrp = '#39'*'#39' )'
      '                        )'
      '           )'
      ''
      '        and FinCrp.CodEmp = FinCre.CodEmp   and'
